<?xml version="1.0" encoding="windows-1252"?>
<?eclipse version="3.0"?>
<plugin>
	<extension
		point="org.eclipse.ui.editors">
		<editor
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.editor.XtextEditor"
			contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
			default="true"
			extensions="dt"
			icon="icons/dt.png"
			id="edu.vanderbilt.riaps.Datatypes"
			name="Datatypes Editor">
		</editor>
	</extension>
	<extension
        point="org.eclipse.ui.commands">
     <command
           defaultHandler="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:edu.vanderbilt.riaps.datatypes.CapnpHandler"
           id="edu.vanderbilt.riaps.datatypes.capnp"
           name="Generate">
     </command>
  </extension>
  	<extension
        point="org.eclipse.ui.commands">
     <command
           defaultHandler="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:edu.vanderbilt.riaps.datatypes.IDLHandler"
           id="edu.vanderbilt.riaps.datatypes.idl"
           name="Generate">
     </command>
  </extension>
  
  <extension
        point="org.eclipse.ui.menus">
     <menuContribution
           allPopups="false"
           locationURI="popup:org.eclipse.ui.popup.any?after=additions">
        <menu
              id="RIAPS"
              label="RIAPS">
           <command
                 commandId="edu.vanderbilt.riaps.datatypes.capnp"
                 label="Generate-Capnp"
                 style="push"
                 tooltip="Generate capnproto files.">
           </command>
              <command
                 commandId="edu.vanderbilt.riaps.datatypes.idl"
                 label="Generate-IDL Files"
                 style="push"
                 tooltip="Generate IDL files.">
           </command>
           <visibleWhen
                 checkEnabled="false">
              <with
                    variable="activeMenuSelection">
                 <iterate>
                 <adapt type="org.eclipse.core.resources.IResource">
             <or>
                <test
                      property="org.eclipse.core.resources.name"
                      value="*.dt">
                </test>
                <test
                      property="org.eclipse.core.resources.name"
                      value="*.dt">
                </test>
             </or>
                 </adapt>
                 </iterate>
              </with>
           </visibleWhen>
        </menu>
     </menuContribution>
  </extension>
	
	<extension
		point="org.eclipse.ui.handlers">
		<handler
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclarationHandler"
			commandId="org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclaration">
			<activeWhen>
				<reference
					definitionId="edu.vanderbilt.riaps.Datatypes.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
		<handler
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.editor.handler.ValidateActionHandler"
			commandId="edu.vanderbilt.riaps.Datatypes.validate">
		<activeWhen>
			<reference
					definitionId="edu.vanderbilt.riaps.Datatypes.Editor.opened">
			</reference>
		</activeWhen>
		</handler>
		<!-- copy qualified name -->
		<handler
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedNameHandler"
			commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName">
			<activeWhen>
				<reference definitionId="edu.vanderbilt.riaps.Datatypes.Editor.opened" />
			</activeWhen>
		</handler>
		<handler
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedNameHandler"
			commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName">
			<activeWhen>
				<and>
					<reference definitionId="edu.vanderbilt.riaps.Datatypes.XtextEditor.opened" />
					<iterate>
						<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
					</iterate>
				</and>
			</activeWhen>
		</handler>
	</extension>
	<extension point="org.eclipse.core.expressions.definitions">
		<definition id="edu.vanderbilt.riaps.Datatypes.Editor.opened">
			<and>
				<reference definitionId="isActiveEditorAnInstanceOfXtextEditor"/>
				<with variable="activeEditor">
					<test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName" 
						value="edu.vanderbilt.riaps.Datatypes" 
						forcePluginActivation="true"/>
				</with>		
			</and>
		</definition>
		<definition id="edu.vanderbilt.riaps.Datatypes.XtextEditor.opened">
			<and>
				<reference definitionId="isXtextEditorActive"/>
				<with variable="activeEditor">
					<test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName" 
						value="edu.vanderbilt.riaps.Datatypes" 
						forcePluginActivation="true"/>
				</with>		
			</and>
		</definition>
	</extension>
	<extension
			point="org.eclipse.ui.preferencePages">
		<page
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
			id="edu.vanderbilt.riaps.Datatypes"
			name="Datatypes">
			<keywordReference id="edu.vanderbilt.riaps.ui.keyword_Datatypes"/>
		</page>
		<page
			category="edu.vanderbilt.riaps.Datatypes"
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.editor.syntaxcoloring.SyntaxColoringPreferencePage"
			id="edu.vanderbilt.riaps.Datatypes.coloring"
			name="Syntax Coloring">
			<keywordReference id="edu.vanderbilt.riaps.ui.keyword_Datatypes"/>
		</page>
		<page
			category="edu.vanderbilt.riaps.Datatypes"
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.editor.templates.XtextTemplatePreferencePage"
			id="edu.vanderbilt.riaps.Datatypes.templates"
			name="Templates">
			<keywordReference id="edu.vanderbilt.riaps.ui.keyword_Datatypes"/>
		</page>
	</extension>
	<extension
			point="org.eclipse.ui.propertyPages">
		<page
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
			id="edu.vanderbilt.riaps.Datatypes"
			name="Datatypes">
			<keywordReference id="edu.vanderbilt.riaps.ui.keyword_Datatypes"/>
			<enabledWhen>
				<adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
			<filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
		</page>
	</extension>
	<extension
		point="org.eclipse.ui.keywords">
		<keyword
			id="edu.vanderbilt.riaps.ui.keyword_Datatypes"
			label="Datatypes"/>
	</extension>
	<extension
		point="org.eclipse.ui.commands">
	<command
			description="Trigger expensive validation"
			id="edu.vanderbilt.riaps.Datatypes.validate"
			name="Validate">
	</command>
	<!-- copy qualified name -->
	<command
			id="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
			categoryId="org.eclipse.ui.category.edit"
			description="Copy the qualified name for the selected element"
			name="Copy Qualified Name">
	</command>
	<command
			id="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName"
			categoryId="org.eclipse.ui.category.edit"
			description="Copy the qualified name for the selected element"
			name="Copy Qualified Name">
	</command>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.edit">
			 <command
				 commandId="edu.vanderbilt.riaps.Datatypes.validate"
				 style="push"
				 tooltip="Trigger expensive validation">
			<visibleWhen checkEnabled="false">
				<reference
					definitionId="edu.vanderbilt.riaps.Datatypes.Editor.opened">
				</reference>
			</visibleWhen>
		</command>
		</menuContribution>
		<!-- copy qualified name -->
		<menuContribution locationURI="popup:#TextEditorContext?after=copy">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName" 
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<reference definitionId="edu.vanderbilt.riaps.Datatypes.Editor.opened" />
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution locationURI="menu:edit?after=copy">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<reference definitionId="edu.vanderbilt.riaps.Datatypes.Editor.opened" />
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution locationURI="popup:org.eclipse.xtext.ui.outline?after=additions">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName" 
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<and>
						<reference definitionId="edu.vanderbilt.riaps.Datatypes.XtextEditor.opened" />
						<iterate>
							<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
						</iterate>
					</and>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution locationURI="popup:#TextEditorContext?endof=group.find">
			<command commandId="org.eclipse.xtext.ui.editor.FindReferences">
				<visibleWhen checkEnabled="false">
					<reference definitionId="edu.vanderbilt.riaps.Datatypes.Editor.opened">
					</reference>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.handlers">
		<handler
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.editor.findrefs.FindReferencesHandler"
			commandId="org.eclipse.xtext.ui.editor.FindReferences">
			<activeWhen>
				<reference
					definitionId="edu.vanderbilt.riaps.Datatypes.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
	<!-- adding resource factories -->
	<extension
		point="org.eclipse.emf.ecore.extension_parser">
		<parser
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.resource.IResourceFactory"
			type="dt">
		</parser>
	</extension>
	<extension point="org.eclipse.xtext.extension_resourceServiceProvider">
		<resourceServiceProvider
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.resource.IResourceUIServiceProvider"
			uriExtension="dt">
		</resourceServiceProvider>
	</extension>
	<!-- marker definitions for edu.vanderbilt.riaps.Datatypes -->
	<extension
			id="datatypes.check.fast"
			name="Datatypes Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.fast"/>
		<persistent value="true"/>
	</extension>
	<extension
			id="datatypes.check.normal"
			name="Datatypes Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.normal"/>
		<persistent value="true"/>
	</extension>
	<extension
			id="datatypes.check.expensive"
			name="Datatypes Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.expensive"/>
		<persistent value="true"/>
	</extension>
	<extension point="org.eclipse.xtext.builder.participant">
		<participant
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.builder.IXtextBuilderParticipant"
			fileExtensions="dt"/>
	</extension>
	<extension point="org.eclipse.ui.preferencePages">
		<page
			category="edu.vanderbilt.riaps.Datatypes"
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.builder.preferences.BuilderPreferencePage"
			id="edu.vanderbilt.riaps.Datatypes.compiler.preferencePage"
			name="Compiler">
			<keywordReference id="edu.vanderbilt.riaps.ui.keyword_Datatypes"/>
		</page>
	</extension>
	<extension point="org.eclipse.ui.propertyPages">
		<page
			category="edu.vanderbilt.riaps.Datatypes"
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.builder.preferences.BuilderPreferencePage"
			id="edu.vanderbilt.riaps.Datatypes.compiler.propertyPage"
			name="Compiler">
			<keywordReference id="edu.vanderbilt.riaps.ui.keyword_Datatypes"/>
			<enabledWhen>
				<adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
			<filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
		</page>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution locationURI="popup:#TextEditorContext?after=xtext.ui.openDeclaration">
			<command
				commandId="org.eclipse.xtext.ui.OpenGeneratedFileCommand"
				id="edu.vanderbilt.riaps.Datatypes.OpenGeneratedCode"
				style="push">
				<visibleWhen checkEnabled="false">
					<reference definitionId="edu.vanderbilt.riaps.Datatypes.Editor.opened" />
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.handlers">
		<handler
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.generator.trace.OpenGeneratedFileHandler"
			commandId="org.eclipse.xtext.ui.OpenGeneratedFileCommand">
			<activeWhen>
				<reference definitionId="edu.vanderbilt.riaps.Datatypes.Editor.opened" />
			</activeWhen>
		</handler>
	</extension>
	<!-- Quick Outline -->
	<extension
		point="org.eclipse.ui.handlers">
		<handler 
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.editor.outline.quickoutline.ShowQuickOutlineActionHandler"
			commandId="org.eclipse.xtext.ui.editor.outline.QuickOutline">
			<activeWhen>
				<reference
					definitionId="edu.vanderbilt.riaps.Datatypes.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
	<extension
		point="org.eclipse.ui.commands">
		<command
			description="Open the quick outline."
			id="org.eclipse.xtext.ui.editor.outline.QuickOutline"
			name="Quick Outline">
		</command>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.open">
			<command commandId="org.eclipse.xtext.ui.editor.outline.QuickOutline"
				style="push"
				tooltip="Open Quick Outline">
				<visibleWhen checkEnabled="false">
					<reference definitionId="edu.vanderbilt.riaps.Datatypes.Editor.opened"/>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<!-- quickfix marker resolution generator for edu.vanderbilt.riaps.Datatypes -->
	<extension
			point="org.eclipse.ui.ide.markerResolution">
		<markerResolutionGenerator
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="edu.vanderbilt.riaps.datatypes.ui.datatypes.check.fast">
			<attribute
				name="FIXABLE_KEY"
				value="true">
			</attribute>
		</markerResolutionGenerator>
		<markerResolutionGenerator
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="edu.vanderbilt.riaps.datatypes.ui.datatypes.check.normal">
			<attribute
				name="FIXABLE_KEY"
				value="true">
			</attribute>
		</markerResolutionGenerator>
		<markerResolutionGenerator
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="edu.vanderbilt.riaps.datatypes.ui.datatypes.check.expensive">
			<attribute
				name="FIXABLE_KEY"
				value="true">
			</attribute>
		</markerResolutionGenerator>
	</extension>
	<!-- Rename Refactoring -->
	<extension point="org.eclipse.ui.handlers">
		<handler 
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.refactoring.ui.DefaultRenameElementHandler"
			commandId="org.eclipse.xtext.ui.refactoring.RenameElement">
			<activeWhen>
				<reference
					definitionId="edu.vanderbilt.riaps.Datatypes.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.edit">
			<command commandId="org.eclipse.xtext.ui.refactoring.RenameElement"
				style="push">
				<visibleWhen checkEnabled="false">
					<reference
						definitionId="edu.vanderbilt.riaps.Datatypes.Editor.opened">
					</reference>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.preferencePages">
		<page
			category="edu.vanderbilt.riaps.Datatypes"
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.refactoring.ui.RefactoringPreferencePage"
			id="edu.vanderbilt.riaps.Datatypes.refactoring"
			name="Refactoring">
			<keywordReference id="edu.vanderbilt.riaps.ui.keyword_Datatypes"/>
		</page>
	</extension>
	<extension point="org.eclipse.compare.contentViewers">
		<viewer id="edu.vanderbilt.riaps.Datatypes.compare.contentViewers"
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.compare.InjectableViewerCreator"
			extensions="dt">
		</viewer>
	</extension>
	<extension point="org.eclipse.compare.contentMergeViewers">
		<viewer id="edu.vanderbilt.riaps.Datatypes.compare.contentMergeViewers"
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.compare.InjectableViewerCreator"
			extensions="dt" label="Datatypes Compare">
		</viewer>
	</extension>
	<extension point="org.eclipse.ui.editors.documentProviders">
		<provider id="edu.vanderbilt.riaps.Datatypes.editors.documentProviders"
			class="edu.vanderbilt.riaps.ui.DatatypesExecutableExtensionFactory:org.eclipse.xtext.ui.editor.model.XtextDocumentProvider"
			extensions="dt">
		</provider>
	</extension>
	<extension point="org.eclipse.team.core.fileTypes">
		<fileTypes
			extension="dt"
			type="text">
		</fileTypes>
	</extension>
</plugin>
